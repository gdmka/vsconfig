{
    "telemetry.telemetryLevel": "off",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "update.mode": "manual",
    "editor.minimap.enabled": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "diffEditor.diffAlgorithm": "advanced",
    "editor.rulers": [
        120
    ],
    "editor.fontFamily": "Monaco, Menlo, 'Courier New', monospace",
    "workbench.iconTheme": "vs-minimal",
    "terminal.integrated.fontSize": 13,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderIndicators": true,
    "editor.autoClosingBrackets": "languageDefined",
    "editor.autoClosingQuotes": "languageDefined",
    "editor.autoSurround": "languageDefined",
    "editor.cursorBlinking": "blink",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "git.enabled": true,
    "go.inferGopath": true,
    "workbench.startupEditor": "none",
    "window.newWindowDimensions": "inherit",
    "explorer.confirmDelete": false,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "extensions.autoCheckUpdates": false,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "debug.allowBreakpointsEverywhere": true,
    "debug.enableAllHovers": true,
    "debug.console.fontSize": 13,
    "debug.openDebug": "neverOpen",
    "zenMode.hideLineNumbers": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.enableExperiments": false,
    "debug.inlineValues": "on",
    "workbench.list.horizontalScrolling": true,
    "rust-client.disableRustup": true,
    "rust-client.enableMultiProjectSetup": true,
    "workbench.settings.openDefaultKeybindings": true,
    "editor.autoClosingOvertype": "never",
    "explorer.confirmDragAndDrop": false,
    "search.searchOnType": true,
    "workbench.colorCustomizations": {},
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ],
    "terminal.integrated.env.windows": {},
    "window.openFoldersInNewWindow": "on",
    "python.analysis.autoImportCompletions": true,
    "python.languageServer": "Pylance",
    "workbench.list.openMode": "singleClick",
    "json.schemas": [],
    "editor.suggestSelection": "first",
    "workbench.editor.wrapTabs": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "search.mode": "newEditor",
    "window.title": "${activeEditorLong}${separator}${rootName}",
    "python.analysis.completeFunctionParens": true,
    "errorLens.severityText": [
        "ERROR",
        "WARNING",
        "INFO",
        "HINT"
    ],
    "errorLens.messageTemplate": "$message",
    "errorLens.enabledDiagnosticLevels": [
        "info"
    ],
    "errorLens.delay": 10,
    "search.showLineNumbers": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.accessibilitySupport": "off",
    "python.experiments.enabled": false,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        },
    },
    "files.insertFinalNewline": true,
    "ruff.nativeServer": "on",
    "ruff.lineLength": 150,
    "ruff.lint.select": [
        "C",
        "E",
        "F",
        "W"
    ],
    "redhat.telemetry.enabled": false,
    "editor.guides.indentation": true,
    "rust.crate_blacklist": [],
    "editor.tokenColorCustomizations": {
        "[*]": {
            "textMateRules": [
                {
                    "name": "Function call",
                    "scope": [
                        "meta.function-call.generic.python",
                        "meta.function-call.python",
                    ],
                    "settings": {
                        "foreground": "#b5645a",
                    },
                },
                {
                    "name": "Parameter variable",
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#51b2b9",
                        "fontStyle": "underline italic"
                    }
                },
                {
                    "name": "Control Flow",
                    "scope": [
                        "keyword.control.flow.python"
                    ],
                    "settings": {
                        "foreground": "#FFCB6B",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Item access/Self",
                    "scope": [
                        "meta.item-access.python",
                        "variable.language.special.self.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#41b421",
                    }
                },
                {
                    "name": "Func params",
                    "scope": [
                        "variable.parameter.function.language.python"
                    ],
                    "settings": {
                        "foreground": "#664bb9",
                        "fontStyle": "italic underline"
                    }
                },
                {
                    "name": "Member access",
                    "scope": [
                        "meta.member.access.python",
                    ],
                    "settings": {
                        "foreground": "#c443c4",
                    }
                },
                {
                    "name": "Const access",
                    "scope": [
                        "constant.language.python",
                    ],
                    "settings": {
                        "foreground": "#807ef8",
                    }
                },
                {
                    "name": "Logical",
                    "scope": [
                        "keyword.operator.logical.python",
                    ],
                    "settings": {
                        "foreground": "#7eabf8",
                    }
                },
                {
                    "name": "Fdef",
                    "scope": [
                        "storage.type.function.python",
                        // "entity.name.function.python"
                    ],
                    "settings": {
                        "foreground": "#82AAFF",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Fname",
                    "scope": [
                        "entity.name.function.python"
                    ],
                    "settings": {
                        "foreground": "#2b6fe3",
                    }
                },
                {
                    "name": "Cdef",
                    "scope": [
                        "storage.type.class.python"
                    ],
                    "settings": {
                        "foreground": "#C792EA",
                    }
                },
                {
                    "name": "Imp",
                    "scope": [
                        "keyword.control.import.python"
                    ],
                    "settings": {
                        "foreground": "#c4aa41",
                        "fontStyle": "underline"
                    }
                },
                {
                    "name": "Support Type",
                    "scope": [
                        "support.type.exception.python",
                        "support.type.python",
                        "support.function.builtin.python"
                    ],
                    "settings": {
                        "foreground": "#89DDFF",
                    }
                },
                {
                    "name": "Self",
                    "scope": [
                        "variable.language.special.self.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
            ]
        }
    },
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.editor.highlightModifiedTabs": true,
    "errorLens.borderRadius": "5px",
    "editor.formatOnSave": true,
    "editor.fontLigatures": false,
    "editor.fontSize": 13,
    "editor.wordWrap": "on",
    "workbench.colorTheme": "Night Owl (No Italics)",
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.startupPrompt": "never",
    "errorLens.lintFilePaths": {
        "eslint": [
            "**/*.eslintrc.{js,cjs,yaml,yml,json}",
            "**/*package.json"
        ],
        "Stylelint": [
            "**/*.stylelintrc",
            "**/*.stylelintrc.{cjs,js,json,yaml,yml}",
            "**/*stylelint.config.{cjs,js}",
            "**/*package.json"
        ]
    },
    "editor.unicodeHighlight.nonBasicASCII": false,
    "window.zoomLevel": 0.5,
    "editor.inlineSuggest.showToolbar": "always",
    "python.defaultInterpreterPath": "",
    "editor.inlineSuggest.suppressSuggestions": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "workbench.editor.enablePreview": false,
    "go.useLanguageServer": true,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "go.formatTool": "goimports",
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "gopls": {
        "ui.semanticTokens": true
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "errorLens.enabled": true,
    "git.openRepositoryInParentFolders": "never",
    "roo-cline.allowedCommands": [
        "git log",
        "git diff",
        "git show"
    ],
    "roo-cline.deniedCommands": [],
    "workbench.activityBar.location": "top",
    "kilo-code.allowedCommands": [
        "git log",
        "git diff",
        "git show",
        "mkdir",
        "git status"
    ],
    "kilo-code.deniedCommands": [],
    "chat.agentSessionsViewLocation": "disabled",
    "chat.mcp.access": "none",
    "telemetry.editStats.enabled": false,
    "telemetry.feedback.enabled": false,
    "chat.disableAIFeatures": true,
    "workbench.secondarySideBar.defaultVisibility": "hidden",
}
