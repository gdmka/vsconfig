{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "update.mode": "manual",
    "editor.minimap.enabled": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.rulers": [
        120
    ],
    "editor.fontFamily": "Monaco, Menlo, 'Courier New', monospace",
    "workbench.iconTheme": "vs-minimal",
    "terminal.integrated.fontSize": 13,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderIndicators": true,
    "editor.autoClosingBrackets": "languageDefined",
    "editor.autoClosingQuotes": "languageDefined",
    "editor.autoSurround": "languageDefined",
    "editor.cursorBlinking": "blink",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "git.enabled": true,
    "go.inferGopath": true,
    "go.useLanguageServer": false,
    "workbench.startupEditor": "none",
    "window.newWindowDimensions": "inherit",
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.loggingLevel": "critical",
    "explorer.confirmDelete": false,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "extensions.autoCheckUpdates": false,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "workbench.sideBar.location": "left",
    "debug.allowBreakpointsEverywhere": true,
    "debug.enableAllHovers": true,
    "debug.console.fontSize": 13,
    "debug.openDebug": "neverOpen",
    "zenMode.hideLineNumbers": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.enableExperiments": false,
    "workbench.activityBar.visible": false,
    "debug.inlineValues": true,
    "workbench.list.horizontalScrolling": true,
    "go.formatTool": "goimports",
    "rust-client.disableRustup": true,
    "rust-client.enableMultiProjectSetup": true,
    "workbench.settings.openDefaultKeybindings": true,
    "editor.autoClosingOvertype": "never",
    "explorer.confirmDragAndDrop": false,
    "search.searchOnType": true,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#AB47BC",
        "activityBar.activeBorder": "#AB47BC",
        "list.activeSelectionForeground": "#AB47BC",
        "list.inactiveSelectionForeground": "#AB47BC",
        "list.highlightForeground": "#AB47BC",
        "scrollbarSlider.activeBackground": "#AB47BC50",
        "editorSuggestWidget.highlightForeground": "#AB47BC",
        "textLink.foreground": "#AB47BC",
        "progressBar.background": "#AB47BC",
        "pickerGroup.foreground": "#AB47BC",
        "tab.activeBorder": "#AB47BC",
        "notificationLink.foreground": "#AB47BC",
        "editorWidget.resizeBorder": "#AB47BC",
        "editorWidget.border": "#AB47BC",
        "settings.modifiedItemIndicator": "#AB47BC",
        "settings.headerForeground": "#AB47BC",
        "panelTitle.activeBorder": "#AB47BC",
        "breadcrumb.activeSelectionForeground": "#AB47BC",
        "menu.selectionForeground": "#AB47BC",
        "menubar.selectionForeground": "#AB47BC",
        "editor.findMatchBorder": "#AB47BC",
        "selection.background": "#AB47BC40",
        "editor.inactiveSelectionBackground": "#f400003b",
        "editor.selectionHighlightBackground": "#2002fff7",
        "editor.wordHighlightBackground": "#2002fff7",
        "editor.selectionBackground": "#f1f90d39",
        "editor.findMatchBackground": "#e1ff02f7"
    },
    "materialTheme.accent": "Purple",
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ],
    "python.formatting.autopep8Args": [
        "max_line_length = 120"
    ],
    "python.linting.flake8CategorySeverity.W": "Error",
    "terminal.integrated.env.windows": {},
    "python.linting.flake8Args": [
        "--max-line-length=120",
        "--ignore=F403,E128,E126,E123,E121, E203, E265,E501,N802,N803,N806,C901, W503"
    ],
    "window.openFoldersInNewWindow": "on",
    "window.openFilesInNewWindow": "on",
    "python.formatting.provider": "black",
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "python.autoComplete.addBrackets": true,
    "python.languageServer": "Pylance",
    "workbench.list.openMode": "doubleClick",
    "jupyter.debugJustMyCode": false,
    "jupyter.jupyterCommandLineArguments": [],
    "jupyter.allowLiveShare": false,
    "json.schemas": [],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editor.wrapTabs": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "search.mode": "newEditor",
    "window.title": "${activeEditorLong}${separator}${rootName}",
    "python.analysis.completeFunctionParens": true,
    "errorLens.annotationPrefix": [
        "ERROR: ",
        "WARNING: ",
        "INFO: ",
        "HINT:"
    ],
    "errorLens.addNumberOfDiagnostics": true,
    "errorLens.addAnnotationTextPrefixes": true,
    "vsintellicode.sql.completionsEnabled": false,
    "terminal.integrated.flowControl": false,
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
    "errorLens.delay": 10,
    "search.showLineNumbers": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "todo-tree.highlights.enabled": false,
    "todo-tree.general.statusBar": "tags",
    "editor.accessibilitySupport": "off",
    "todo-tree.tree.buttons.scanMode": true,
    "python.experiments.enabled": false,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter-notebook",
            "filenamePattern": "*.ipynb"
        }
    ],
    "workbench.colorTheme": "Palenight Theme",
    "files.insertFinalNewline": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "jupyter.useNotebookEditor": false,
    "redhat.telemetry.enabled": false,
    "editor.guides.indentation": true,
    "rust.crate_blacklist": [],
    // "gitlens.remotes": null,
    "editor.tokenColorCustomizations": {
        "[Palenight Theme]": {
            "textMateRules": [
                {
                    "name": "Function call",
                    "scope": [
                        "meta.function-call.generic.python",
                        "punctuation.definition.arguments.begin.python",
                        "meta.function-call.python",
                        "meta.member.access.python",
                        "punctuation.definition.arguments.end.python"
                    ], /*functions()*/
                    "settings": {
                        "foreground": "#e26f60"
                    }
                }
            ]
        }
        // "gitlens.remotes": null,
    },
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.editor.highlightModifiedTabs": true,
    "errorLens.borderRadius": "5px",
    "gitlens.advanced.messages": {
        "suppressRebaseSwitchToTextWarning": true
    },
    "editor.formatOnSave": true,
    "window.zoomLevel": 0.5,
    "editor.fontLigatures": false
}
